	<!--Definición de los datos locales de Ayuda correspondiente a la tabla BEC_AYUDAS-->
	<xs:element name="AyudTipoAyuda">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:length value="2"/>
				<xs:pattern value="\d\d"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el código de la ayuda genérica. Tabla BEC-AYUDAS-->
		<!--Es de tipo string y tiene que tener dos dígitos-->
	</xs:element>
	<xs:element name="AyudCodAyuda">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:minLength value="1"/>
				<xs:maxLength value="2"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el código de la ayuda. Tabla BEC-AYUDAS-->
		<!--Es de tipo string-->
	</xs:element>
	<xs:element name="AyudCuantia">
		<xs:simpleType>
			<xs:restriction base="xs:decimal">
				<xs:totalDigits value="8"/>
				<xs:fractionDigits value="2"/>
				<xs:maxInclusive value="999999.99"/>
				<xs:minInclusive value="0"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el importe de la ayuda. Tabla BEC-AYUDAS-->
		<!--Es de tipo decimal con 8 dígitos, 2 decimales y solo puede ser positiva-->
	</xs:element>
	<xs:element name="AyudCuantiaSuple">
		<xs:simpleType>
			<xs:restriction base="xs:decimal">
				<xs:totalDigits value="8"/>
				<xs:fractionDigits value="2"/>
				<xs:maxInclusive value="999999.99"/>
				<xs:minInclusive value="0"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el importe de la ayuda suplementaria. Tabla BEC-AYUDAS-->
		<!--Es de tipo decimal con 8 dígitos, 2 decimales y solo positivo-->
	</xs:element>
	<xs:element name="AyudIndPropUT">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="N"/>
				<xs:enumeration value="S"/>
				<xs:enumeration value="E"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Indicador de si la ayuda ha sido propuesta por la UT: N (No), S (Sí), E (Eliminada) -->
	</xs:element>
	<xs:element name="AyudIndPropCentro">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:length value="1"/>
				<xs:enumeration value="N"/>
				<xs:enumeration value="S"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Indicador de si la ayuda ha sido propuesta por el Centro (Educación Especial): N (No), S (Sí) -->
	</xs:element>
	<xs:element name="AyudIndConcedida">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="N"/>
				<xs:enumeration value="S"/>
				<xs:enumeration value="P"/>
				<xs:enumeration value="R"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Indicador de si la ayuda ha sido concedida por SSCC: N (No), S (Sí), P (Pendiente de Pago en una solicitud Pagada Parcial), R (Renuncia) -->
	</xs:element>
	<xs:element name="AyudIdOrdenPago">
		<xs:simpleType>
			<xs:restriction base="xs:unsignedInt">
				<xs:minInclusive value="1"/>
				<xs:maxInclusive value="999999"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica la Orden de Pago en la cual se pagó la ayuda. Tabla BEC-AYUDAS-->
		<!--Es de tipo numerico con un máximo de 6 dígitos-->
	</xs:element>
	<xs:element name="AyudCuantiaAPagar">
		<xs:simpleType>
			<xs:restriction base="xs:decimal">
				<xs:totalDigits value="8"/>
				<xs:fractionDigits value="2"/>
				<xs:maxInclusive value="999999.99"/>
				<xs:minInclusive value="-999999.99"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el importe a pagar para esa ayuda. Tabla BEC-AYUDAS-->
		<!--Es de tipo decimal con 8 dígitos, 2 decimales y puede ser positiva o negativa-->
	</xs:element>
	<!--Definición de los datos locales de CabeceraEnvio correspondiente a la tabla BEC_CAB_ENV y BEC_CAB_ACT-->
	<xs:element name="CaenIdCurso">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:length value="4"/>
				<xs:pattern value="\d\d\d\d"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el año inicial del curso académico-->
	</xs:element>
	<xs:element name="CaenCodConv">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="AE"/>
				<xs:enumeration value="AM"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el código de la convocatoria de becas-->
		<!--Es de tipo string con 2 caracteres: AE, AM, ....-->
	</xs:element>
	<xs:element name="CaenSecuenc">
		<xs:simpleType>
			<xs:restriction base="xs:unsignedInt">
				<xs:minInclusive value="1"/>
				<xs:maxInclusive value="2"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Número de secuencia para una misma convocatoria en un año. Valores: 1 Universitarios, Masteres y Otros Estudios Superiores, 2 Enseñanzas Medias-->
	</xs:element>
	<xs:element name="CaenTipoUTTramite">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="1"/>
				<xs:enumeration value="2"/>
				<xs:enumeration value="3"/>
				<xs:enumeration value="4"/>
			</xs:restriction>
			<!--Identifica el tipo de UT, 1:Universidad, 2: Provincia, 3: CCAA, 4:MECD-->
		</xs:simpleType>
	</xs:element>
	<xs:element name="CaenCodUTTramite">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:length value="3"/>
				<xs:pattern value="\d\d\d"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el código de la UT de trámite-->
		<!--Es de tipo string con 3 dígitos-->
	</xs:element>
	<xs:element name="CaenLote">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:length value="5"/>
				<xs:pattern value="\d\d\d\d\d"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el número de lote generado en la UT con el formato con el formato 99999, que es un contador que se inicializa anualmente y es independiente de la convocatoria-->
		<!--Es de tipo string con 5 dígitos-->
	</xs:element>
	<xs:element name="CaenCodSituacEnvio">
		<xs:simpleType>
			<xs:restriction base="xs:unsignedInt">
				<xs:totalDigits value="1"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica la situación del lote enviado, 1:Cargado en XML, 2: Cargado en BBDD, 3: Retenido, 4: Descargado, 5: Actualizac.temporal, 6: Generado, 7: Enviado a SSCC, 8: Enviado a UT-->
	</xs:element>
	<xs:element name="CaenFechaGen" type="xs:date"/>
	<!--Identifica la fecha de generación del fichero (UT o SSCC) en formato AAAA-MM-DD-->
	<xs:element name="CaenFechaEnv" type="xs:date"/>
	<!--Identifica la fecha de envío (UT o SSCC) y que viene en la etiqueta en formato AAAA-MM-DD-->
	<xs:element name="CaenFechaRecep" type="xs:date"/>
	<!--Identifica la fecha de recepción de la etiqueta en formato AAAA-MM-DD-->
	<xs:element name="CaenTotSoli">
		<xs:simpleType>
			<xs:restriction base="xs:unsignedInt">
				<xs:minInclusive value="0"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el total de solicitudes enviadas tal y como se indica en la etiqueta-->
		<!--Es de tipo numérico con un valor mínimo de 0-->
	</xs:element>
	<xs:element name="CaenFechaBD" type="xs:date"/>
	<!--Identifica la fecha de carga en BD en formato AAAA-MM-DD-->
	<xs:element name="CaenNumSoliErr">
		<xs:simpleType>
			<xs:restriction base="xs:unsignedInt">
				<xs:minInclusive value="0"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el número de solicitudes erróneas dentro del lote y cuyo error se graba en BEC_ERRORES-->
		<!--Es de tipo numérico con un valor mínimo de 0-->
	</xs:element>
	<xs:element name="CaenNumSoliCarg">
		<xs:simpleType>
			<xs:restriction base="xs:unsignedInt">
				<xs:minInclusive value="0"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el número de solicitudes leídas y cargadas en BD-->
		<!--Es de tipo numérico con un valor mínimo de 0-->
	</xs:element>
	<xs:element name="CaenTipoEnvio">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="0"/>
				<xs:enumeration value="1"/>
				<xs:enumeration value="2"/>
				<xs:enumeration value="3"/>
				<xs:enumeration value="4"/>
				<xs:enumeration value="5"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el tipo de envío: 0:Alta, 1: Aumento de cuantía, 2: Altas y aumentos de cuantía mezclados (solo SSCC), 3: Migración, 4: Grabadas en SSCC, 5: No gestionadas en SSCC-->
	</xs:element>
	<xs:element name="CaenTipoUTGen">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="1"/>
				<xs:enumeration value="2"/>
				<xs:enumeration value="3"/>
				<xs:enumeration value="4"/>
			</xs:restriction>
			<!--Identifica el tipo de UTde generación, 1:Universidad, 2: Provincia, 3: CCAA, 4:MECD-->
		</xs:simpleType>
	</xs:element>
	<xs:element name="CaenCodUTGen">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:length value="3"/>
				<xs:pattern value="\d\d\d"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el código de la UT de generación. La UT que realiza/recibe los envíos-->
		<!--Es de tipo string con 3 dígitos-->
	</xs:element>
	<xs:element name="CaenLoteGen">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:length value="5"/>
				<xs:pattern value="\d\d\d\d\d"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el número de lote de generación con el formato con el formato 99999, que es un contador que se inicializa anualmente. Es independiente de la convocatoria-->
		<!--Es de tipo string con 5 dígitos-->
	</xs:element>
