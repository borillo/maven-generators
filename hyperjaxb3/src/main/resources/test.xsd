			<xs:restriction base="xs:unsignedInt">
				<xs:minInclusive value="1"/>
				<xs:maxInclusive value="999999"/>
				<xs:totalDigits value="6"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el número de orden de pago donde se pagó la solicitud-->
	</xs:element>
	<xs:element name="OrpaImporte">
		<xs:simpleType>
			<xs:restriction base="xs:decimal">
				<xs:totalDigits value="12"/>
				<xs:fractionDigits value="2"/>
				<xs:maxInclusive value="9999999999.99"/>
				<xs:minInclusive value="0"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Importe total que se corresponde con el total pagado en esa orden de pago-->
	</xs:element>
	<xs:element name="OrpaTotalBec">
		<xs:simpleType>
			<xs:restriction base="xs:unsignedInt">
				<xs:minInclusive value="1"/>
				<xs:maxInclusive value="999999"/>
				<xs:totalDigits value="6"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el número total de becarios en la orden de pago-->
	</xs:element>
	<xs:element name="OrpaFechaEnvTeso" type="xs:date"/>
	<!--Fecha de envío al Tesoro de la Orden de Pago-->
	<xs:element name="OrpaFechaPagoReal" type="xs:date"/>
	<!--Fecha en que el Tesoro Comunica que las solicitudes de la orden de pago han sido pagadas-->
	<!--Fin de la definición del tipo complejo OrdenPago -->
	<!--Definición de los datos locales de Error correspondiente a la tabla BEC_ERRORES-->
	<xs:element name="ErroCodTiperrg">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:length value="5"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el código de error-->
	</xs:element>
	<xs:element name="ErroFecha" type="xs:date"/>
	<!--Fecha del error-->
	<xs:element name="ErroIdCurso">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:length value="4"/>
				<xs:pattern value="\d\d\d\d"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el año inicial del curso académico-->
	</xs:element>
	<xs:element name="ErroCodConv">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:length value="2"/>
				<xs:enumeration value="AE"/>
				<xs:enumeration value="AM"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el código de la convocatoria. AE General, AM Movilidad-->
	</xs:element>
	<xs:element name="ErroSecuenc">
		<xs:simpleType>
			<xs:restriction base="xs:unsignedInt">
				<xs:minInclusive value="1"/>
				<xs:maxInclusive value="2"/>
				<xs:totalDigits value="1"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Número de secuencia para una misma convocatoria en un año. Valores: 1 Universitarios, Masteres y Otros Estudios Superiores, 2 Enseñanzas Medias-->
	</xs:element>
	<xs:element name="ErroTipoUTTramite">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="1"/>
				<xs:enumeration value="2"/>
				<xs:enumeration value="3"/>
				<xs:enumeration value="4"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el tipo de UT de trámite: 1 (Universidad), 2 (Provincia), 3 (CCAA) y 4 (MECD)-->
	</xs:element>
	<xs:element name="ErroCodUTTramite">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:length value="3"/>
				<xs:pattern value="\d\d\d"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el código de la UT de trámite-->
	</xs:element>
	<xs:element name="ErroIdSolicitud">
		<xs:simpleType>
			<xs:restriction base="xs:unsignedInt">
				<xs:minInclusive value="1"/>
				<xs:maxInclusive value="99999999"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Número de solicitud-->
	</xs:element>
	<xs:element name="ErroCodTiso">
		<xs:simpleType>
			<xs:restriction base="xs:unsignedInt">
				<xs:minInclusive value="0"/>
				<xs:maxInclusive value="99"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica si se trata de una solicitud (0) o un aumento de cuantía (número del aumento de cuantía)-->
	</xs:element>
	<xs:element name="ErroCodDocuIdent">
		<xs:simpleType>
			<xs:restriction base="xs:unsignedInt">
				<xs:minInclusive value="1"/>
				<xs:maxInclusive value="2"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el tipo de documento de indentificación: 1 (NIF), 2 (NIE)-->
	</xs:element>
	<xs:element name="ErroNif">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="15"/>
				<xs:minLength value="9"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Nif del solicitante-->
	</xs:element>
	<xs:element name="ErroNombre">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="24"/>
				<xs:minLength value="1"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Nombre del solicitante-->
	</xs:element>
	<xs:element name="ErroApellido1">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="24"/>
				<xs:minLength value="1"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Primer apellido-->
	</xs:element>
	<xs:element name="ErroApellido2">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="24"/>
				<xs:minLength value="1"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Segundo apellido-->
	</xs:element>
	<xs:element name="ErroError">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="100"/>
				<xs:minLength value="1"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Descripción del error con el nombre del campo de la solicitud que es errónea-->
	</xs:element>
	<xs:element name="ErroLote">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:length value="5"/>
				<xs:pattern value="\d\d\d\d\d"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el número de lote generado en la UT con el formato con el formato 99999, que es un contador que se inicializa anualmente-->
		<!--Es de tipo string con 5 dígitos-->
	</xs:element>
	<xs:element name="ErroLoteSSCC">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:length value="5"/>
				<xs:pattern value="\d\d\d\d\d"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el número de lote generado en SSCC con el formato con el formato 99999, que es un contador que se inicializa anualmente-->
		<!--Es de tipo string con 5 dígitos-->
	</xs:element>
	<xs:element name="ErroTipoUTGen">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:enumeration value="1"/>
				<xs:enumeration value="2"/>
				<xs:enumeration value="3"/>
				<xs:enumeration value="4"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica el tipo de UT de generación: 1 (Universidad), 2 (Provincia), 3 (CCAA) y 4 (MECD)-->
		<!--Es de tipo string con 1 dígito-->
	</xs:element>
	<xs:element name="ErroCodUTGen">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:length value="3"/>
				<xs:pattern value="\d\d\d"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Identifica la UT de generación-->
		<!--Es de tipo string con 3 dígitos-->
	</xs:element>
	<xs:element name="ErroDeslError">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:minLength value="1"/>
				<xs:maxLength value="500"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Descripción larga del error-->
	</xs:element>
	<!--Fin de la definición del tipo complejo Error -->
	<xs:element name="ErroClave">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:minLength value="1"/>
				<xs:maxLength value="8"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Clave aplicación externa-->
	</xs:element>
	<xs:element name="ErroSecErr">
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:minLength value="1"/>
				<xs:maxLength value="5"/>
			</xs:restriction>
		</xs:simpleType>
		<!--Número de secuencia del registro erróneo correspondiente a la solicitud que se carga a partir del XML-->
	</xs:element>
	<!--Fin de la definición del tipo complejo Error -->
</xs:schema>
